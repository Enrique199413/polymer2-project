{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-about.html",
    "src/my-career.html",
    "src/my-hobbies.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "js": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "css": {
        "minify": true
      }
    }
  ]
}
